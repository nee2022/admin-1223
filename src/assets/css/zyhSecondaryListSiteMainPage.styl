.zyhSecondaryListSiteMainPage {
  margin: 0;
  padding: 0;
  box-sizing: border-box;

  *, *::before, *::after {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
  }

  li {
    list-style-position: inside;
    list-style-type: none;
  }

  .clearfix::after {
    content: '';
    display: block;
    clear: both;
  }

  height: 100vh;
  display: flex;
  flex-flow: column;

  header {
    height: 102px;

    .infoArea {
      display: flex;
      flex-direction: row;

      .pageName {
        flex-grow: 1;
        line-height: 102px;
        text-indent: 38px;
        font-size: 24px;
        letter-spacing: 1px;
        font-family: PingFangSC-Regular;
        color: #000000;
      }

      .profile {
        margin-top: 40px;
        margin-right: 37px;
      }
    }
  }

  section {
    flex-grow: 1;
    display: flex;
    flex-direction: row;

    .asidePart {
      height: calc(100vh - 116px);
      width: calc((100vw - 200px) * 0.335);

      .wrapper {
        height: 100%;
        width: 361px !important;
        margin-left: 50px;
        box-shadow: 1px 1px 20px 10px #edf1f5;
        display: flex;
        flex-direction: column;

        // border: 1px solid red;
        .el-table {
          flex-grow: 1;

          // border: 1px solid red;
          .el-table__header {
            .searchArea {
              height: 36px;
              display: flex;
              flex-direction: row;

              // border: 1px solid red;
              .inputFrame {
                flex-grow: 1;
                margin: 0 15px;
                height: 36px;
                border-radius: 10px;
                border: solid 1px #1e69fe;
                display: flex;
                flex-direction: row;
                align-items: center;
                justify-content: space-around;

                img {
                  margin-left: 20px;
                  margin-right: 0px;
                }

                .inputBlank {
                  height: 30px;

                  .el-input__inner {
                    height: 30px;
                    text-indent: 10px;
                    border: none;
                  }
                }
              }

              .addButton {
                // border: 1px solid green;
                margin: 0 15px;

                .el-button {
                  border-radius: 10px;
                  padding: 0 15px;
                  height: 36px;
                  font-size: 16px;
                  background: #1e69fe;
                  color: #fff;
                  display: flex;
                  justify-content: center;
                  align-items: center;
                }
              }
            }
          }

          .el-table__body {
            tr {
              height: calc(((100vh - 260px) / 6));

              .cell {
                // border: 1px solid black;
                display: flex;

                .siteIcon {
                  flex-grow: 1;

                  img {
                    height: calc((100vh - 260px) / 6 - 30px);
                  }
                }

                .siteInfo {
                  width: 230px;
                  // border: 1px solid black;
                  font-size: 16px;
                  color: #908e8e;
                  display: flex;
                  flex-direction: column;
                  align-items: flex-start;
                  justify-content: space-around;

                  .name {
                    // border: 1px solid red;
                    // height: 30px;
                    // line-height: 30px;
                  }

                  .address {
                    height: calc((100vh - 260px) / 6 - 80px);
                    line-height: calc((100vh - 260px) / 6 - 80px);
                  }

                  .usageDetail {
                    font-size: 13px;
                    color: #5f6062;
                    // height: 22px;
                    // line-height: 22px;
                    display: flex;
                    flex-direction: row;

                    span {
                      background: #f2d0bd;
                      border: 1px solid #f2d0bd;
                      margin-right: 2px;
                      width: 50px;
                      border-radius: 6px;
                    }
                  }
                }
              }
            }
          }
        }

        .pageNumArea {
          display: flex;
          align-items: center;
          justify-content: space-between;
          margin: 0 30px;
          height: 100%;
          max-height: 85px;

          .total {
            font-family: PingFangSC-Regular;
            font-size: 14px;
            font-weight: 600;
            color: #333333;
          }
        }
      }
    }

    .mainPart {
      .wrapper {
        width: calc((100vw - 200px) * 0.665);
        height: 100%;
        display: flex;
        flex-flow: column;

        .searchArea {
          height: 36px;
          display: flex;
          flex-direction: row;
          margin-left: 38px;

          .inputFrame {
            width: 442px;
            height: 36px;
            border-radius: 10px;
            border: solid 1px #1e69fe;
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: space-around;

            img {
              margin-left: 20px;
              margin-right: 0px;
            }

            .inputBlank {
              width: 390px;
              height: 30px;

              .el-input__inner {
                height: 30px;
                text-indent: 10px;
                border: none;
              }
            }
          }

          .searchButton {
            .el-button {
              border-radius: 10px;
              width: 95px;
              height: 36px;
              margin-left: 8px;
              font-size: 16px;
              background: #1e69fe;
              color: #fff;
            }
          }

          .addButton {
            .el-button {
              border-radius: 10px;
              width: 95px;
              height: 36px;
              margin-left: 8px;
              font-size: 16px;
              background: #fff;
              color: #1e69fe;
            }
          }
        }

        .el-table {
          flex-grow: 1;

          .el-table__header {
            tr {
              height: 60px;

              .cell {
                text-align: center;
                font-size: 16px;
                font-weight: 400;
                color: black;
              }
            }
          }

          .el-table__body {
            tr {
              height: calc(((100vh - 288px) / 12));

              .cell {
                text-align: center;
                font-size: 16px;
                font-weight: 400;
                color: #908e8e;

                .operation {
                  display: flex;
                  flex-direction: row;
                  justify-content: space-between;
                  width: 70px;
                  margin: 0 auto;
                }
              }
            }
          }
        }

        .pageNumArea {
          display: flex;
          align-items: center;
          justify-content: space-between;
          margin: 0 30px;
          height: 100%;
          max-height: 90px;

          .total {
            font-family: PingFangSC-Regular;
            font-size: 16px;
            font-weight: 600;
            color: #333333;
          }
        }
      }
    }
  }
}
